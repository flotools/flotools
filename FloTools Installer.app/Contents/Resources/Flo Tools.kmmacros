function KMInit() {
str = window.KeyboardMaestro.GetVariable('FTPreference')
console.log("Input: "+str)
window.preference = JSON.parse(str)
window.setTimeout(set, 100)
}

function set() {
for (key in window.preference) {
value = window.preference[key]
if (typeof(value) == typeof(true)) {
selector = "input[name='"+key+"']"
console.log(selector)
window.document.querySelector(selector).checked = value
continue
}
selector = "input[name='"+key+"'][value='"+value+"']"
console.log(selector)
element = window.document.querySelector(selector)
if (element != null) {
element.checked = true
continue
}
selector = "select[name='"+key+"']"
console.log(selector)
element = window.document.querySelector(selector)
if (element != null) {
for (i=0; i&lt;element.options.length; i++) {
if (element.options[i].value == value) {
element.options[i].selected = true
break
}
}
continue
}

}
}

function ok() {
selector = "input,select"
settings = window.document.querySelectorAll(selector)
for (s in settings) {
element = settings[s]
if (element.tagName == null) continue
console.log("Name: "+element.name+", tag: "+element.tagName);
if (element.type == "checkbox") {
window.preference[element.name] = element.checked
} else if (element.type == "radio" &amp; element.checked == true) {
window.preference[element.name] = element.value
} else if (element.tagName == "SELECT") {
window.preference[element.name] = element.options[element.selectedIndex].value
}
}
str = JSON.stringify(window.preference)
console.log("Output: "+str)
window.KeyboardMaestro.SetVariable( 'FTPreference', str)
window.KeyboardMaestro.Cancel()
}

function cancel() {
window.KeyboardMaestro.Cancel()
}

&lt;/script&gt;

&lt;div id='preference'&gt;
&lt;p&gt;
&lt;input name="temporary inspector" type="checkbox" /&gt;Use temporary inspector&lt;br/&gt;
&lt;input name="starts with Flo Tools inspector" type="checkbox" /&gt;Turn on the Flo Tools inspector when Pro Tools starts&lt;br/&gt;
Doubletap Timeout: 
&lt;select name="timeout"&gt;
&lt;option value="200"&gt;200&lt;/option&gt;
&lt;option value="300"&gt;300&lt;/option&gt;
&lt;option value="400"&gt;400&lt;/option&gt;
&lt;option value="500"&gt;500&lt;/option&gt;
&lt;/select&gt;
&lt;/p&gt;

&lt;p&gt;
&lt;button onClick="cancel()"&gt;CANCEL&lt;/button&gt;
&lt;button onClick="ok()"&gt;OK&lt;/button&gt;
&lt;/p&gt;
&lt;/div&gt;</string>
