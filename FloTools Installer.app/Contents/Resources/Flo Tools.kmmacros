function single() {
vo = Application("VoiceOver")
sys = Application("System Events")
pt = sys.applicationProcesses.whose({name:{_contains:"Pro Tools"}})[0]
windows = pt.windows
edit = windows.whose({name:{_beginsWith:"Edit:"}})[0]
value = edit.groups.byName("Edit Mode Cluster").buttons.byName("EditModeSlip").value()
if (value == "Selected") vo.output ("Slip Mode, On")
else vo.output("Slip Mode, Off")
}

