kme.playSound(Path("/System/Library/Sounds/Tink.aiff"))
double()
} else  {
runSingle = true
while (new Date().getTime()-start&lt;kme.getvariable("FTTimeout")) {
command = kme.getvariable("command").split(".")
if (currentCommand == command[0] &amp; command[1]&gt;start) {
runSingle=false
break;
}
}
if (runSingle) single()
}
