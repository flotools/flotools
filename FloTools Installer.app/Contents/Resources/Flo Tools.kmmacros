command = kme.getvariable("command")
if (x&gt;tlx&amp;x&lt;brx&amp;y&gt;tly&amp;y&lt;bry) {
vo.output(command)
} else {
vo.output("Object is outside the window, "+window.name())
x=-1
y=-1
}

kme.setvariable("x", {to:""+x})
kme.setvariable("y", {to:""+y})
